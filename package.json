  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "mongoose": "^4.10.5",
    "mongoose-mongodb-errors": "0.0.2",
    "passport": "^0.3.2",
    "passport-local-mongoose": "^4.0.0",
    "request": "^2.81.0",
    "socket.io": "^2.0.2",
    "validator": "^7.0.0"
  },
  "xo": {
    "esnext": false,
    "ignore": [
      "public/js/build.js"
